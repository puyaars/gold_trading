{
  "name": "gold_trading",
  "packageManager": "npm@8.1.0",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema"
  },
  "dependencies": {
    "@prisma/client": "3.5.0",
    "@quixo3/prisma-session-store": "^3.1.0",
    "apollo-server-express": "^3.5.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "graphql": "^15.7.2",
    "graphql-middleware": "^6.1.12",
    "graphql-passport": "^0.6.4",
    "graphql-scalars": "^1.13.6",
    "graphql-shield": "^7.5.0",
    "graphql-upload": "^12.0.0",
    "jsonwebtoken": "^8.5.1",
    "minio": "^7.0.23",
    "nanoid": "^3.1.30",
    "nexus": "^1.1.0",
    "nexus-plugin-shield": "^0.2.0",
    "p-iteration": "^1.1.8",
    "passport": "^0.5.0",
    "ramda": "^0.27.1",
    "redis": "^4.0.0",
    "rxjs": "^7.4.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/graphql-upload": "^8.0.7",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/minio": "^7.0.11",
    "@types/node": "^16.11.10",
    "@types/ramda": "^0.27.59",
    "@types/redis": "^2.8.32",
    "prisma": "3.5.0",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2"
  }
}
